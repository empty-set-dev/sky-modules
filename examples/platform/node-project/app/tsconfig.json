{
    "compilerOptions": {
        "strict": true,
        "lib": [
            "es2022",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "ES2022",
        "target": "ES2017",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "typeRoots": [
            "../node_modules/sky/node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "pkgs/*": [
                "../node_modules/sky/pkgs/*"
            ],
            "defines/*": [
                "../.dev/defines/*"
            ],
            "sky/*": [
                "../node_modules/sky/*"
            ],
            "#/*": [
                "./*"
            ],
            "app/*": [
                "./*"
            ]
        }
    },
    "include": [
        "../sky.config.ts",
        "../deploy.ts",
        "../node_modules/sky/pkgs",
        "../.dev/defines",
        "../node_modules/sky",
        ".",
        "."
    ],
    "exclude": [
        "../node_modules/sky/node_modules",
        "../node_modules"
    ]
}