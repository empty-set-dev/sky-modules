{
    "compilerOptions": {
        "strict": true,
        "lib": [
            "es2022",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "esnext",
        "target": "es2017",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "pkgs/*": [
                "pkgs/*"
            ],
            "defines/*": [
                ".dev/defines/*"
            ],
            "sky/*": [
                "./*"
            ],
            "#/*": [
                "commands/assets/web-initial/*"
            ]
        }
    },
    "include": [
        "."
    ],
    "exclude": [
        "node_modules"
    ]
}