{
    "compilerOptions": {
        "strict": true,
        "lib": [
            "es2022",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "ES2022",
        "target": "ES2017",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "pkgs/*": [
                "pkgs/*"
            ],
            "defines/*": [
                "../../defines/*"
            ],
            "sky/*": [
                "./*"
            ],
            "#/*": [
                "../../../../sky-modules/commands/assets/web-initial/*"
            ]
        }
    },
    "include": [
        "../../../sky.config.ts",
        "../../../deploy.ts",
        "pkgs",
        "../../defines",
        ".",
        "../../../../sky-modules/commands/assets/web-initial"
    ],
    "exclude": [
        "node_modules",
        "../../../node_modules"
    ]
}